{
    "name": "job-sphere",
    "private": true,
    "workspaces": [
        "frontend",
        "backend"
    ],
    "scripts": {
        "install": "npm install --workspaces",
        "start": "concurrently \"npm start --prefix frontend\" \"npm start --prefix backend\"",
        "dev": "concurrently \"npm run dev --prefix frontend\" \"npm run dev --prefix backend\""
    }
}
